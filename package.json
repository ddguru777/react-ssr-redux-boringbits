{
  "name": "boringbits",
  "version": "1.0.0",
  "description": "Frameworks should be boring, applications they build should be exciting",
  "main": "dist/boring.js",
  "scripts": {
    "test": "mocha node_modules/mocha/bin/_mocha --require @babel/register $(find ./src -name '*-test.js')",
    "test-debug": "node --inspect-brk=9229 node_modules/mocha/bin/_mocha  --require @babel/register $(find ./src -name '*-test.js')",
    "test-watch": "nodemon --watch src --exec \"npm test && npm run build\"",
    "build": "babel src -d dist --source-maps",
    "prepublish": "npm run build",
    "meep": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanstevens/boring.git"
  },
  "author": "Ryan Stevens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanstevens/boring/issues"
  },
  "homepage": "https://github.com/ryanstevens/boring#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "config": "^2.0.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "glob": "^7.1.3",
    "mocha": "^5.2.0",
    "proxyquire": "^2.1.0"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "caller": "^1.0.1",
    "deepmerge": "^2.1.1",
    "eventemitter2": "^5.0.1",
    "express": "^4.16.3",
    "nodemon": "^1.18.4",
    "serve-static": "^1.13.2",
    "understudy": "^4.1.0"
  },
  "nodemon": {
    "ignoreRoot": [
      ".git"
    ]
  }
}
