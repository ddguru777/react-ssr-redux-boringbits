{
  "name": "boringbits",
  "version": "1.1.0",
  "description": "Frameworks should be boring, applications they build should be exciting",
  "main": "dist/boring.js",
  "private": true,
  "scripts": {
    "test": "mocha node_modules/mocha/bin/_mocha --require @babel/register $(find ./src -name '*-test.js')",
    "test-debug": "node --inspect-brk=9229 node_modules/mocha/bin/_mocha  --require @babel/register $(find ./src -name '*-test.js')",
    "test-watch": "nodemon --watch src --exec \"npm test | npx bunyan\"",
    "test-build": "nodemon --watch src --exec \"npm test && npm run build\"",
    "build": "rm -rf dist && babel src -d dist --source-maps",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "boring-build-node": "./bin/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanstevens/boring.git"
  },
  "author": "Ryan Stevens",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ryanstevens/boring/issues"
  },
  "homepage": "https://github.com/ryanstevens/boring#readme",
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "proxyquire": "^2.1.0"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "babel-preset-react": "^6.24.1",
    "bunyan": "^1.8.12",
    "caller": "^1.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "compose-middleware": "^5.0.0",
    "config": "^2.0.1",
    "css-loader": "^1.0.0",
    "deepmerge": "^2.1.1",
    "eventemitter2": "^5.0.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "healthy": "^1.0.2",
    "nodemon": "^1.18.4",
    "postcss-loader": "^3.0.0",
    "react-dev-utils": "^5.0.2",
    "react-hot-loader": "^4.3.6",
    "serve-static": "^1.13.2",
    "style-loader": "^0.23.0",
    "understudy": "^4.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "nodemon": {
    "ignoreRoot": [
      ".git"
    ]
  }
}
