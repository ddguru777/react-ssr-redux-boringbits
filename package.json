{
  "name": "boringbits",
  "version": "3.0.0",
  "description": "Frameworks should be boring, applications they build should be exciting",
  "main": "dist/boring.js",
  "scripts": {
    "test": "NODE_ENV=unit-tests mocha node_modules/mocha/bin/_mocha --require @babel/register $(find ./src -name '*-test.js')",
    "test-debug": "node --inspect-brk=9229 node_modules/mocha/bin/_mocha  --require @babel/register $(find ./src -name '*-test.js')",
    "test-watch": "nodemon --watch src --exec \"npm test | npx bunyan\"",
    "test-build": "nodemon --watch src --exec \"npm test && npm run build\"",
    "build": "rm -rf dist && babel src -d dist --source-maps",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "boring-build-node": "./bin/build-node.js",
    "boring-build-client": "./bin/build-client.js",
    "boring-up": "./bin/docker-compose-up.js",
    "boring-down": "./bin/docker-compose-down.js"
  },
  "author": "Ryan Stevens",
  "license": "UNLICENSED",
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "proxyquire": "^2.1.0"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@material-ui/core": "^3.5.1",
    "async": "^2.6.1",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "caller": "^1.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "compose-middleware": "^5.0.0",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "css-loader": "^1.0.0",
    "deepmerge": "^2.1.1",
    "dotenv": "^6.0.0",
    "eventemitter2": "^5.0.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "healthy": "^1.0.2",
    "mini-css-extract-plugin": "^0.4.2",
    "moment": "^2.22.2",
    "nodemon": "^1.18.4",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.6.3",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.6",
    "react-jss": "^8.6.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.1",
    "serve-static": "^1.13.2",
    "style-loader": "^0.23.0",
    "understudy": "^4.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "nodemon": {
    "ignoreRoot": [
      ".git"
    ]
  }
}
